<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bjb.upmfx.MainWinController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <SplitPane dividerPositions="0.5" layoutX="249.0" layoutY="90.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="691.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <StackPane layoutX="181.0" layoutY="17.0" prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <VBox fx:id="connectingVBox" prefHeight="200.0" prefWidth="100.0" spacing="10.0" visible="false">
                                 <children>
                                    <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                                       <children>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Port" />
                                                <ChoiceBox fx:id="portChoiceBox" prefHeight="26.0" prefWidth="111.0" />
                                             </children>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Baud" />
                                                <ChoiceBox fx:id="baudrateChoiceBox" prefWidth="150.0" />
                                             </children>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Bits" />
                                                <ChoiceBox fx:id="dataBitsChoiceBox" prefWidth="150.0" />
                                             </children>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Stop" />
                                                <ChoiceBox fx:id="stopBitsChoiceBox" prefWidth="150.0" />
                                             </children>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Parity" />
                                                <ChoiceBox fx:id="parityChoiceBox" prefWidth="150.0" />
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                       <children>
                                          <Button fx:id="connectButton" defaultButton="true" mnemonicParsing="false" onAction="#onConnect" text="Connect" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                              <VBox fx:id="connectedVBox" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                                 <children>
                                    <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0">
                                       <children>
                                          <Label text="Pump Speed" />
                                          <Slider fx:id="pumpSpeedSlider" blockIncrement="1.0" majorTickUnit="10.0" minorTickCount="9" showTickLabels="true" showTickMarks="true" snapToTicks="true" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                    <HBox spacing="15.0">
                                       <children>
                                          <Label text="Celsius" />
                                          <Slider fx:id="celsiusSlider" blockIncrement="1.0" majorTickUnit="10.0" minorTickCount="9" showTickLabels="true" showTickMarks="true" snapToTicks="true" HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                    <Separator prefWidth="200.0" />
                                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                       <children>
                                          <Label text="Celsius" textAlignment="RIGHT" />
                                          <Label fx:id="celsiusLabel" text="50C" />
                                          <Label text="Pump Speed%" textAlignment="RIGHT" />
                                          <Label fx:id="pumpSpeedLabel" text="10%" />
                                          <Label text="PWM Enabled" textAlignment="RIGHT" />
                                          <Label fx:id="pwmEnabledLabel" text="yes" />
                                          <Label text="Duty Cycle %" textAlignment="RIGHT" />
                                          <Label fx:id="dutyCycleLabel" text="20%" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                                       <children>
                                          <Button fx:id="disconnectButton" defaultButton="true" mnemonicParsing="false" onAction="#onDisconnect" text="Disconnect" />
                                       </children>
                                    </HBox>
                                 </children>
                                 <StackPane.margin>
                                    <Insets />
                                 </StackPane.margin>
                                 <padding>
                                    <Insets left="20.0" right="20.0" top="20.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </StackPane>
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <ListView fx:id="messageListView" prefHeight="200.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        <HBox prefWidth="200.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                           <children>
                              <Label fx:id="connectionStatusLabel" text="Label" />
                              <Region prefHeight="5.0" prefWidth="20.0" HBox.hgrow="ALWAYS" />
                              <Label fx:id="systemStatusLabel" text="Label" />
                           </children>
                           <padding>
                              <Insets left="20.0" right="20.0" />
                           </padding>
                        </HBox>
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
