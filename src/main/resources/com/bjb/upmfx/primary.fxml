<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ChoiceBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bjb.upmfx.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="primaryButton" text="Switch to Secondary View" onAction="#switchToSecondary"/>
      <ChoiceBox fx:id="serialPortsChoiceBox" onShowing="#onSerialPortsShowing" onAction="#onSerialPortAction" layoutX="44.0" layoutY="71.0" prefHeight="25.0" prefWidth="191.0"/>
      <ChoiceBox fx:id="baudrateChoiceBox" onAction="#onBaudrateAction" prefHeight="25.0" prefWidth="191.0"/>
      <ChoiceBox fx:id="parityChoiceBox" onAction="#onParityAction" prefHeight="25.0" prefWidth="191.0"/>
      <ChoiceBox fx:id="stopBitsChoiceBox" onAction="#onStopBitsAction" prefHeight="25.0" prefWidth="191.0"/>
      <ChoiceBox fx:id="dataBitsChoiceBox" onAction="#onDataBitsAction" prefHeight="25.0" prefWidth="191.0"/>
      
      
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
